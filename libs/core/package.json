{
  "name": "@poketto/core",
  "version": "0.0.1",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -cw",
    "prebuild": "yarn clean",
    "dts": "tsc --declaration --emitDeclarationOnly",
    "clean": "rimraf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "typing": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "sideEffects": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.es.js"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/node": "^18.0.5",
    "@types/react": "^18.0.15",
    "aptos": "^1.3.6",
    "react": "^18.2.0",
    "rollup": "^2.78.1",
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "aptos": "^1.3.6",
    "react": "^18.2.0"
  },
  "dependencies": {
    "@metamask/browser-passworder": "^3.0.0",
    "@scure/bip39": "^1.1.0",
    "buffer": "^6.0.3",
    "tweetnacl": "^1.0.3"
  }
}
