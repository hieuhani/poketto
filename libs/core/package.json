{
  "name": "@poketto/core",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite build --watch",
    "prebuild": "yarn clean",
    "build": "vite build && yarn dts",
    "dts": "tsc --declaration --emitDeclarationOnly",
    "clean": "rimraf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "typing": "./dist/index.d.ts",
  "main": "./dist/core.js",
  "module": "./dist/core.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/core.js",
      "import": "./dist/core.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^18.0.5",
    "@types/react": "^18.0.15",
    "@vitejs/plugin-react": "^2.0.1",
    "aptos": "^1.3.6",
    "react": "^18.2.0",
    "vite": "^3.0.9",
    "vite-plugin-react": "^4.0.1"
  },
  "peerDependencies": {
    "aptos": "^1.3.6",
    "react": "^18.2.0"
  },
  "dependencies": {
    "@metamask/browser-passworder": "^3.0.0",
    "@scure/bip39": "^1.1.0",
    "buffer": "^6.0.3",
    "tweetnacl": "^1.0.3"
  }
}
